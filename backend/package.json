  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts"
  },
  "keywords": [
    "confeitaria",
    "backend",
    "typescript",
    "projeto-final",
    "autenticação",
    "jwt",
    "prisma",
    "postgresql",
    "multer",
    "upload-de-imagens",
    "api-rest",
    "gerenciamento-de-produtos",
    "delícias-da-tia",
    "nodejs"
  ],
  "author": "Thalia Matos",
  "license": "ISC",
